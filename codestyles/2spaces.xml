<code_scheme name="2spaces">
  <ClojureCodeStyleSettings>{
  :cursive.formatting/align-conditionals false
  :cursive.formatting/align-maps false
}</ClojureCodeStyleSettings>
  <CssCodeStyleSettings>
    <option name="BLANK_LINES_BETWEEN_BLOCKS" value="0" />
    <option name="KEEP_SINGLE_LINE_BLOCKS" value="true" />
  </CssCodeStyleSettings>
  <JSCodeStyleSettings>
    <option name="USE_SEMICOLON_AFTER_STATEMENT" value="false" />
    <option name="ALIGN_OBJECT_PROPERTIES" value="1" />
    <option name="SPACE_BEFORE_FUNCTION_LEFT_PARENTH" value="false" />
    <option name="REFORMAT_C_STYLE_COMMENTS" value="false" />
  </JSCodeStyleSettings>
  <JSON>
    <option name="OBJECT_WRAPPING" value="1" />
    <option name="ARRAY_WRAPPING" value="1" />
  </JSON>
  <MarkdownNavigatorCodeStyleSettings>
    <option name="RIGHT_MARGIN" value="72" />
  </MarkdownNavigatorCodeStyleSettings>
  <SqlCodeStyleSettings>
    <option name="KEYWORD_CASE" value="1" />
  </SqlCodeStyleSettings>
  <XML>
    <option name="XML_LEGACY_SETTINGS_IMPORTED" value="true" />
  </XML>
  <codeStyleSettings language="CSS">
    <indentOptions>
      <option name="INDENT_SIZE" value="2" />
      <option name="CONTINUATION_INDENT_SIZE" value="4" />
      <option name="TAB_SIZE" value="2" />
    </indentOptions>
  </codeStyleSettings>
  <codeStyleSettings language="JavaScript">
    <option name="KEEP_BLANK_LINES_IN_CODE" value="1" />
    <option name="SPACE_BEFORE_WHILE_PARENTHESES" value="false" />
    <option name="SPACE_BEFORE_FOR_PARENTHESES" value="false" />
    <option name="KEEP_SIMPLE_BLOCKS_IN_ONE_LINE" value="true" />
    <option name="KEEP_SIMPLE_METHODS_IN_ONE_LINE" value="true" />
    <indentOptions>
      <option name="INDENT_SIZE" value="2" />
      <option name="CONTINUATION_INDENT_SIZE" value="2" />
      <option name="TAB_SIZE" value="2" />
    </indentOptions>
  </codeStyleSettings>
</code_scheme>